    "#random numbers\n",
    "#Take between 1 to 100\n",
    "\n",
    "import random\n",
    "s=0\n",
    "l=[]\n",
    "for i in range(10):\n",
    "    n=random.randint(1,100)\n",
    "    l.append(n)\n",
    "    s+=n\n",
    "print(\"RANDOM NUMBERS\")\n",
    "for j in l:\n",
    "    print(j,end=\"  \")\n",
    "print(\"\\n\")\n",
    "print(\"Sum=\", s)"
   ]
  }
